package Array;

public class min {
    public static void main(String args[]){
        int[] arr = { 59,25,65,29,88,96,99};
        int min = arr[0];
     for(int i = 0; i<arr.length; i++){
        if(arr[i]<min){
            min= arr[i];
        }
    }
    System.out.print("The minimum number in the array element are : "+ min);
    }
}
