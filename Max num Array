package Array;

public class max {
    public static void main(String args[]){
        int[] arr = { 100,25,65,29,88,96,99};
        int max = 0;
        for(int i = 0; i<arr.length; i++){
            if(arr[i]>max){
                max = arr[i];
            }
        }
        System.out.print("The maximum number in the array element are : "+ max);

    }
}
